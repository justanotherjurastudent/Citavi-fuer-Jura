    <ComponentPart id="bf010e28-b66c-41f0-a82f-995d19762082">
      <AllFieldElementsRequired>false</AllFieldElementsRequired>
      <FilterCode><![CDATA[using System.Linq;
using System.Collections.Generic;
using SwissAcademic.Citavi;
using SwissAcademic.Citavi.Metadata;
using SwissAcademic.Collections;
using SwissAcademic.Drawing;

namespace SwissAcademic.Citavi.Citations
{
	public class ComponentPartFilter
		:
		IComponentPartFilter
	{
		public IEnumerable<ITextUnit> GetTextUnits(ComponentPart componentPart, Template template, Citation citation, out bool handled)
		{
			//Diese Komponente wirkt NUR auf das erste PageRange Feldelement:
			//Fall [1] Seitenangaben des Artikels ausblenden, wenn Startseite Artikel identisch mit erster Zitatseite
			//Fall [2] Suffixe ausblenden und nur Startseite zeigen, wenn Zitatseiten vorhanden
			
			handled = false;
			
			if (componentPart == null) return null;
			if (componentPart.Elements == null || !componentPart.Elements.Any()) return null;
			
			PageRangeFieldElement articlePageRangeFieldElement = componentPart.Elements.OfType<PageRangeFieldElement>().Where(element => element.PropertyId == ReferencePropertyId.PageRange).FirstOrDefault();
			if (articlePageRangeFieldElement == null) return null;
			
			if (citation == null || citation.Reference == null) return null;
			if (!citation.Reference.HasCoreField(ReferenceTypeCoreFieldId.PageRange)) return null;
			
			PageRange articlePageRange = citation.Reference.PageRange;
			if (articlePageRange == null || articlePageRange.StartPage == null) return null;
			
			string articleStartPage = articlePageRange.StartPage.PrettyString;
			if (string.IsNullOrEmpty(articleStartPage)) return null;
			
			PlaceholderCitation placeholderCitation = citation as PlaceholderCitation;
			if (placeholderCitation == null) return null;
			
			PageRange quotationPageRange = placeholderCitation.PageRange;
			if (quotationPageRange == null || quotationPageRange.StartPage == null) return null;
			string quotationStartPage = quotationPageRange.StartPage.PrettyString;
			bool hasQuotationPageRange = !string.IsNullOrEmpty(quotationPageRange.PrettyString);
			
			if (hasQuotationPageRange)
			{
				//Fall [1]
				if (quotationStartPage.Equals(articleStartPage))
				{
					componentPart.Elements.Remove(articlePageRangeFieldElement);
				}
				else
				{
					//Fall [2]
					articlePageRangeFieldElement.PageMultiNumberingStyle = NumberingStyle.StartPageOnly;
					articlePageRangeFieldElement.PageMultiSuffix.Text = null;
					articlePageRangeFieldElement.PageOneHasSpecialFormat = false;
					articlePageRangeFieldElement.PageTwoHasSpecialFormat = false;
					
					articlePageRangeFieldElement.ColumnMultiNumberingStyle = NumberingStyle.StartPageOnly;
					articlePageRangeFieldElement.ColumnMultiSuffix.Text = null;
					articlePageRangeFieldElement.ColumnOneHasSpecialFormat = false;
					articlePageRangeFieldElement.ColumnTwoHasSpecialFormat = false;
					
					articlePageRangeFieldElement.MarginMultiNumberingStyle = NumberingStyle.StartPageOnly;
					articlePageRangeFieldElement.MarginMultiSuffix.Text = null;
					articlePageRangeFieldElement.MarginOneHasSpecialFormat = false;
					articlePageRangeFieldElement.MarginTwoHasSpecialFormat = false;
					
					articlePageRangeFieldElement.OtherMultiNumberingStyle = NumberingStyle.StartPageOnly;
					articlePageRangeFieldElement.OtherMultiSuffix.Text = null;
					articlePageRangeFieldElement.OtherOneHasSpecialFormat = false;
					articlePageRangeFieldElement.OtherTwoHasSpecialFormat = false;
					
					articlePageRangeFieldElement.ParagraphMultiNumberingStyle = NumberingStyle.StartPageOnly;
					articlePageRangeFieldElement.ParagraphMultiSuffix.Text = null;
					articlePageRangeFieldElement.ParagraphOneHasSpecialFormat = false;
					articlePageRangeFieldElement.ParagraphTwoHasSpecialFormat = false;
				}	
				
			}

			return null;
		}
	}
}]]></FilterCode>
