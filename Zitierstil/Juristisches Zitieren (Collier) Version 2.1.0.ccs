			foreach(ITextUnit textUnit in textUnits)
			{
				if (textUnit.Text.Contains("Ergänzungslieferung"))
				{
					found = true;
					textUnit.Text = textUnit.Text.Replace("Ergänzungslieferung", "EL.");
				
