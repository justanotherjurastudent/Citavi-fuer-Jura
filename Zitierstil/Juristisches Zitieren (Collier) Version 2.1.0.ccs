    <CustomTemplateCondition id="9d0df143-4e71-452d-a760-04ecc962bb7a">
      <Name>Kurzbeleg = Nachname des Autors</Name>
      <Notes />
      <Macro><![CDATA[//TXY004 - var: Field CITATION KEY = Authors' last name
//Check, whether CitationKey matches one of the authors' last name

using System;
using System.Linq;
using System.Collections.Generic;
using SwissAcademic.Citavi;
using SwissAcademic.Citavi.Metadata;
using SwissAcademic.Collections;

namespace SwissAcademic.Citavi.Citations
{
	public class CustomTemplateCondition : ITemplateConditionMacro
	{
		public bool IsTemplateForReference(ConditionalTemplate template, Citation citation)
		{
			//If you do not wish the citation key to be checked as the publisher's last name of the superordinate work,
			//then enter"= false;" in the following line:
			bool checkParentReference = true;


			if (citation == null) return false;
			if (citation.Reference == null) return false;
			if (citation.Reference.CitationKey == null) return false;
			

			//Set a variable to check if the citation key matches one of the authors' last name more comfortable
			string lastNameCitationKey = citation.Reference.CitationKey;

			var authors = citation.Reference.AuthorsOrEditorsOrOrganizations;
			var hasAuthor = hasPerson(authors, lastNameCitationKey);

			if (hasAuthor) return true;

			var parentReference = citation.Reference.ParentReference;
			if (parentReference == null) return false;


			if (checkParentReference)
			{
				var editors = parentReference.AuthorsOrEditorsOrOrganizations;
				var hasEditor = hasPerson(editors, lastNameCitationKey);

				if (hasEditor) return true;
			}
			else
			{
				return false;
			}

			return false;
		}


		private bool hasPerson(IEnumerable<Person> persons, string lastNameCitationKey)
		{
			foreach (Person person in persons)
			{
				if (string.Compare(person.LastName, lastNameCitationKey, StringComparison.Ordinal) == 0)
				{
					return true;
				}
			}

			return false;
	
		}
	}
}]]></Macro>
    </CustomTemplateCondition>
